#include <stdio.h>

int main(void)
{
    int year;

    printf("연도를 입력하시오: ");
    scanf("%d",&year);

    if(year % 4 == 0){
        if(year % 100 != 0 || year % 400 == 0){
            printf("result = ture");
        }
        else{
            printf("result = false");
            }
    
    }
    return 0;
}
